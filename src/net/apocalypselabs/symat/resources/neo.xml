<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<!--
        Dark theme based off of Notepad++'s Obsidian theme.
        See theme.dtd and org.fife.ui.rsyntaxtextarea.Theme for more information.
-->
<RSyntaxTheme version="1.0">

    <!-- Omitting baseFont will use a system-appropriate monospaced. -->
    <!--<baseFont family="..." size="13"/>-->
   
    <!--  General editor colors. -->
    <background color="293134"/>
    <caret color="00ff00"/>
    <selection useFG="false" bg="404E51" roundedEdges="false"/>
    <currentLineHighlight color="2F393C" fade="false"/>
    <marginLine fg="00ff00"/>
    <markAllHighlight color="6b8189"/> <!-- TODO: Fix me -->
    <markOccurrencesHighlight color="5b7179" border="false"/>
    <matchedBracket fg="00ff00" bg="6b8189" highlightBoth="false" animate="true"/>
    <hyperlinks fg="00ff00"/>
    <secondaryLanguages>
        <language index="1" bg="334455"/>
        <language index="2" bg="223322"/>
        <language index="3" bg="ffe0f0"/>
    </secondaryLanguages>
   
    <!-- Gutter styling. -->
    <gutterBorder color="81969A"/>
    <lineNumbers fg="00ff00"/>
    <foldIndicator fg="00ff00" iconBg="2f383c"/>
    <iconRowHeader activeLineRange="3399ff"/>
   
    <!-- Syntax tokens. -->
    <tokenStyles>
        <style token="IDENTIFIER" fg="00ff00"/>
        <style token="RESERVED_WORD" fg="00ff00" bold="true"/>
        <style token="RESERVED_WORD_2" fg="00ff00" bold="true"/>
        <style token="ANNOTATION" fg="00ff00"/>
        <style token="COMMENT_DOCUMENTATION" fg="00ff00"/>
        <style token="COMMENT_EOL" fg="00ff00"/>
        <style token="COMMENT_MULTILINE" fg="00ff00"/>
        <style token="COMMENT_KEYWORD" fg="00ff00"/>
        <style token="COMMENT_MARKUP" fg="00ff00"/>
        <style token="FUNCTION" fg="00ff00"/>
        <style token="DATA_TYPE" fg="00ff00" bold="true"/>
        <style token="LITERAL_BOOLEAN" fg="00ff00" bold="true"/>
        <style token="LITERAL_NUMBER_DECIMAL_INT" fg="00ff00"/>
        <style token="LITERAL_NUMBER_FLOAT" fg="00ff00"/>
        <style token="LITERAL_NUMBER_HEXADECIMAL" fg="00ff00"/>
        <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="00ff00"/>
        <style token="LITERAL_CHAR" fg="00ff00"/>
        <style token="LITERAL_BACKQUOTE" fg="00ff00"/>
        <style token="MARKUP_TAG_DELIMITER" fg="00ff00"/>
        <style token="MARKUP_TAG_NAME" fg="00ff00" bold="true"/>
        <style token="MARKUP_TAG_ATTRIBUTE" fg="00ff00"/>
        <style token="MARKUP_TAG_ATTRIBUTE_VALUE" fg="00ff00"/>
        <style token="MARKUP_COMMENT" fg="00ff00"/>
        <style token="MARKUP_DTD" fg="00ff00"/>
        <style token="MARKUP_PROCESSING_INSTRUCTION" fg="00ff00"/>
        <style token="MARKUP_CDATA" fg="00ff00"/>
        <style token="MARKUP_CDATA_DELIMITER" fg="00ff00"/>
        <style token="MARKUP_ENTITY_REFERENCE" fg="00ff00"/>
        <style token="OPERATOR" fg="00ff00"/>
        <style token="PREPROCESSOR" fg="00ff00"/>
        <style token="REGEX" fg="00ff00"/>
        <style token="SEPARATOR" fg="00ff00"/>
        <style token="VARIABLE" fg="00ff00" bold="true"/>
        <style token="WHITESPACE" fg="00ff00"/>
      
        <style token="ERROR_IDENTIFIER" fg="00ff00" bg="04790e"/>
        <style token="ERROR_NUMBER_FORMAT" fg="00ff00" bg="04790e"/>
        <style token="ERROR_STRING_DOUBLE" fg="00ff00" bg="04790e"/>
        <style token="ERROR_CHAR" fg="00ff00" bg="04790e"/>
    </tokenStyles>

</RSyntaxTheme>
